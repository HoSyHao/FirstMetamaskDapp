{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.js",
        "use": "@vercel/static"
      },
      {
        "src": "*.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "${https://firstmetamaskdapp.onrender.com/api}/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }